<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>


<SplitPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="com.example.tourplanner.UI.View.SearchBarController"
           dividerPositions="0.6" stylesheets="@stylesheets/searchBar.css, @stylesheets/buttonOrange.css">
    <TextField fx:id="searchInputField" />
    <Button text="search" fx:id="searchButton" onAction="#searchButtonClicked" styleClass="myButton" />
    <Button text="clear" fx:id="clearButton" onAction="#clearSearchField" styleClass="myButton"/>
</SplitPane>

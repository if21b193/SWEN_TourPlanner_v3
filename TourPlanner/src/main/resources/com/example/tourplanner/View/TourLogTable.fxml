<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.tourplanner.UI.View.TourLogTableController"
            prefHeight="400.0" prefWidth="600.0"
            fx:id="tourLogTablePane">

    <TableView fx:id="tourLogTable">
        <columns>
            <TableColumn fx:id="dateColumn" text="Date"/>
            <TableColumn fx:id="commentColumn" text="Comment"/>
            <TableColumn fx:id="difficultyColumn" text="Difficulty"/>
            <TableColumn fx:id="timeColumn" text="Actual Time"/>
            <TableColumn fx:id="ratingColumn" text="Rating"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>

</Pane>
